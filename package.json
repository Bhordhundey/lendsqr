{
  "name": "lendsqr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev ./src/server.ts",
    "build": "tsc",
    "start": "NODE_ENV=default node ./dist/src/server.js",
    "test": " NODE_ENV=test nyc  --reporter text   ts-mocha './src/tests/**/*.spec.ts' --exit",
    "test:watch": "npm run test -- --watchAll",
    "knex:migrate:make": "knex migrate:make -x ts --migrations-directory ./src/config/db/migrations",
    "knex:migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --knexfile ./src/config/db/knexfile.ts" ,
    "knex:migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:rollback --knexfile ./src/config/db/knexfile.ts"
  },
  "keywords": [],
