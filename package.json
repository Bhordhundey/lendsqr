    "crypto-random-string": "^3.3.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "mysql": "^2.18.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.5",
    "uuid": "^9.0.0"
